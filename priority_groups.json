{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/main/schema/latest/as3-schema.json",
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.0.0",
        "id": "prio_groups_test",
        "label": "prio_groups_test",
        "prio_groups_test": {
            "class": "Tenant",
            "prio_groups_test": {
                "class": "Application",
                "vs_http": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "192.168.57.72"
                    ],
                    "persistenceMethods": [],
                    "profileHTTP": {
                        "use": "pr_http_xff"
                    },
                    "pool": "pl_ltm-demo_prio_groups",
                    "snat": {
                        "use": "pl_SNAT_addresses"
                    }
                },
                "pr_http_xff": {
                    "class": "HTTP_Profile",
                    "xForwardedFor": true
                },
                "pl_ltm-demo_prio_groups": {
                    "class": "Pool",
                    "loadBalancingMode": "least-connections-member",
                    "monitors": ["http"],
                    "minimumMembersActive": 3,
                    "members": [
                        {
                            "servicePort": 8000,
                            "priorityGroup": 10,
                            "serverAddresses": [
                                "10.0.2.71",
                                "10.0.2.72",
                                "10.0.2.73",
                                "10.0.2.74"
                            ],
                            "shareNodes": true
                        },
                        {
                            "servicePort": 8000,
                            "priorityGroup": 5,
                            "serverAddresses": [],
                            "shareNodes": true
                        }
                    ]
                },
                "pl_SNAT_addresses": {
                    "class": "SNAT_Pool",
                    "snatAddresses": [
                        "10.0.2.250"
                    ]
                }
            }
        }
    }
}